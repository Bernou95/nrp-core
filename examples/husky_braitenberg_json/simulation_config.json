{
	"SimulationName": "test_sim",
	"SimDescription": "Launches a Gazebo simulation with a Husky robot and a Nest sim with a Braitenberg model",
	"SimulationTimeout": 20,
	"EngineConfigs": [
		{
			"EngineType": "gazebo_json",
			"EngineName": "gazebo",
			"GazeboWorldFile": "husky_world.sdf",
			"EngineProcStartParams": ["--verbose"]
		},
		{
			"EngineType": "nest_json",
			"EngineName": "nest",
			"NestInitFileName": "braitenberg.py",
			"EngineEnvParams": ["PYNEST_QUIET=1"]
		}
	],
	"DeviceProcessingFunctions": [
		{
			"Name": "cam_tf",
			"FileName": "cam_tf.py"
		},
		{
			"Name": "mot_tf",
			"FileName": "mot_tf.py"
		}
	]
}
