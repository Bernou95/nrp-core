{
	"SimulationName": "husky_simulation",
	"SimDescription": "Launches a Gazebo simulation with a Husky robot and a Nest sim with a Braitenberg model which controls the robot",
	"SimulationTimeout": 20,
	"EngineConfigs": [
		{
			"EngineType": "gazebo_json",
			"EngineName": "gazebo",
			"EngineProcStartParams": ["--verbose"],
			"GazeboWorldFile": "/share/experiment_data/gazebo/husky_world.sdf",
			"ServerAddress":"localhost:6666",
			"RegistrationServerAddress":"localhost:8008",
			"EngineLaunchCommand":{
				"LaunchType":"DockerLauncher",
				"DaemonAddress":"localhost:5002",
				"ImageName":"nrp_gazebo:v0",
				"EngineEnv":[
					"LD_LIBRARY_PATH=/root/.local/nrp/lib:/usr/lib/x86_64-linux-gnu:/usr/lib/x86_64-linux-gnu/gazebo-11/plugins:/root/.local/nrp/lib/nrp_gazebo_plugins:/usr/lib/x86_64-linux-gnu/gazebo-11/plugins:$LD_LIBRARY_PATH",
					"PYTHONPATH=/root/.local/nrp/lib/python3.8/site-packages:$PYTHONPATH",
					"DISPLAY=:1"
				],
				"UploadFolder":".",
				"EngineCmd":"/usr/bin/gzserver",
				"WorkDir": "/root"
			}
		},
		{
			"EngineType": "nest_json",
			"EngineName": "nest",
			"NestInitFileName": "braitenberg.py",
			"EngineEnvParams": ["PYNEST_QUIET=1"]
		}
	],
	"DataPackProcessingFunctions": [
		{
			"Name": "cam_pf",
			"FileName": "cam_pf.py"
		},
		{
			"Name": "mot_tf",
			"FileName": "mot_tf.py"
		},
		{
			"Name": "brain_stimulation_tf",
			"FileName": "brain_stimulation_tf.py"
		}
	]
}
