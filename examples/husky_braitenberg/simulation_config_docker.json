{
	"SimulationName": "husky_simulation",
	"SimDescription": "Launches a Gazebo simulation with a Husky robot and a Nest sim with a Braitenberg model which controls the robot",
	"SimulationTimeout": 20,
	"EngineConfigs": [
		{
			"EngineType": "gazebo_grpc",
			"EngineName": "gazebo",
			"GazeboWorldFile": "husky_world.sdf",
			"EngineProcessLauncher":{"ProcStartParams": ["--verbose"],
			"ProcCmd": "xvfb-run /usr/bin/gzserver",
			"LaunchCommand":{
				"LaunchType":"DockerLauncher",
				"DockerDaemonAddress":"unix:///var/run/docker.sock",
				"ImageName":"nrp-core/nrp-gazebo-ubuntu20:latest"
			}
            }
		},
		{
			"EngineType": "nest_json",
			"EngineName": "nest",
			"NestInitFileName": "braitenberg.py",
			"EngineProcessLauncher":{"ProcEnvParams": ["PYNEST_QUIET=1"]}
		}
	],
	"DataPackProcessingFunctions": [
		{
			"Name": "cam_pf",
			"FileName": "cam_pf.py"
		},
		{
			"Name": "mot_tf",
			"FileName": "mot_tf.py"
		},
		{
			"Name": "brain_stimulation_tf",
			"FileName": "brain_stimulation_tf.py"
		}
	]
}
