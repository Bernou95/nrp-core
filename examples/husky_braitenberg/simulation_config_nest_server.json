{
	"SimulationName": "husky_simulation_nest_server",
	"SimDescription": "Launches a Gazebo simulation with a Husky robot and a Nest sim with a Braitenberg model which controls the robot. Uses the nest_server engine.",
	"SimulationTimeout": 20,
	"EngineConfigs": [
		{
			"EngineType": "gazebo_grpc",
			"EngineName": "gazebo",
			"GazeboWorldFile": "husky_world.sdf",
			"EngineProcessLauncher":{"ProcStartParams": ["--verbose"]}
		},
		{
			"EngineType": "nest_server",
			"EngineName": "nest",
			"NestInitFileName": "braitenberg_nest_server.py",
			"EngineProcessLauncher":{"ProcEnvParams": ["PYNEST_QUIET=1"]}
		}
	],
	"DataPackProcessingFunctions": [
		{
			"Name": "cam_pf",
			"FileName": "cam_pf.py"
		},
		{
			"Name": "mot_tf",
			"FileName": "mot_tf.py"
		},
		{
			"Name": "brain_stimulation_tf",
			"FileName": "brain_stimulation_tf.py"
		}
	]
}
